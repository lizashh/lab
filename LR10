int main() {
	setlocale(LC_ALL, "Russian");

	int a, b, c;
	cout << "Введите значение стороны A: ";
	cin >> a;
	cout << "Введите значение стороны B: ";
	cin >> b;
	cout << "Введите значение стороны C: ";
	cin >> c;

	if ((a + b) > c && (a + c) > b && (b + c) > a) {
		cout << "Существует";
	}
	else {
		cout << "Не существует";
	}
}
