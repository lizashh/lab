#include <iostream>
using namespace std;

//задание1
int main()
{
	setlocale(LC_ALL, "Rus");
	int A, B, i, i2;
	cout << "Введите значения А и В (А<B)" << endl;
	cout << "A=";
	cin >> A;
	cout << "B=";
	cin >> B;
	for(i = A; i <= B; i++)
		for(i2 = 1; i2 <= i; i2++)
		cout << i << endl;
}

