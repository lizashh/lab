//задание3
int main()
{
	setlocale(LC_ALL, "Rus");
	int N;
	cout << "Введите N: ";
	cin >> N;
	int K = 0;
	int s = 0;
	while (s < N)
	{
		K++;
		s += K;
	}
	cout << "Наименьшее число равно: " << K << endl;
	cout << "Сумма равна: " << s << endl;
}
