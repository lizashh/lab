#include <iostream>
using namespace std;
//задание1 
int main() {
	setlocale(LC_ALL, "Rus");
	int a, b;
	cout << "Введите номер дня от 1 до 31 ";
	cin >> a;
	cout << "Введите номер месяца от 1 до 12 ";
	cin >> b;
	if (a > 19 && a % 10 != 0)
	{
		if (a / 10 == 2)
			cout << "двадцать ";
		else
			if (a / 10 == 3)
				cout << "тридцать ";
	}
	else
		if (a == 20)
			cout << "двадцатое";
		else
			if (a == 30)
				cout << "тридцатое";
	if (a < 9 || a > 19)
	{
		if (a % 10 == 1)
			cout << "первое";
		if (a % 10 == 2)
			cout << "второе";
		if (a % 10 == 3)
			cout << "третье";
		if (a % 10 == 4)
			cout << "четвертое";
		if (a % 10 == 5)
			cout << "пятое";
		if (a % 10 == 6)
			cout << "шестое";
		if (a % 10 == 7)
			cout << "седьмое";
		if (a % 10 == 8)
			cout << "восьмое";
		if (a % 10 == 9)
			cout << "девятое";

	}
	else
	{
		if (a == 11)
			cout << "одинадцатое";
		if (a == 12)
			cout << "двенадцатое";
		if (a == 13)
			cout << "тринадцатое";
		if (a == 14)
			cout << "четырнадцатое";
		if (a == 15)
			cout << "пятнадцатое";
		if (a == 16)
			cout << "шестнадцатое";
		if (a == 17)
			cout << "семнадцатое";
		if (a == 18)
			cout << "восемнадцатое";
		if (a == 19)
			cout << "девятнадцатое";
	}
	if (b == 1)
		cout << " января" << endl;
	if (b == 2)
		cout << " февраля" << endl;
	if (b == 3)
		cout << " марта" << endl;
	if (b == 4)
		cout << " апреля" << endl;
	if (b == 5)
		cout << " мая" << endl;
	if (b == 6)
		cout << " июня" << endl;
	if (b == 7)
		cout << " июля" << endl;
	if (b == 8)
		cout << " августа" << endl;
	if (b == 9)
		cout << " сентября" << endl;
	if (b == 10)
		cout << " октября" << endl;
	if (b == 11)
		cout << " ноября" << endl;
	if (b == 12)
		cout << " декабря" << endl;
}

